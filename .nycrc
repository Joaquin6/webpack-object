{
  "description": "Webpack Object Code Coverage Summary",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "watermarks": {
      "lines": [80, 95],
      "functions": [80, 95],
      "branches": [80, 95],
      "statements": [80, 95]
    },
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [ "babel-register" ],
    "cache": true,
    "all": true
  }
}
